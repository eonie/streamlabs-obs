{
  "extends": ["tslint-config-prettier", "tslint-plugin-prettier"],
  "rules": {
    "max-line-length": {
      "options": [120]
    },
    "prettier": true,
    "quotemark": [true, "single", "avoid-escape", "avoid-template"],
    "curly": [true, "ignore-same-line"],
    "no-consecutive-blank-lines": [true, 2]
  },
  "jsRules": {
  }
}

